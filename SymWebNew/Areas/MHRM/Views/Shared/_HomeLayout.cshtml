<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>ShampanERP-@ViewBag.Title</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width" />
  <script src="~/assets/javascripts/jquery.min.js"></script>
    <script src="~/assets/javascripts/jquery-1.8.2.min.js"></script>
    <script src="~/assets/javascripts/jquery-ui-1.8.24.min.js"></script>

     <link href="~/assets/css/jquery-ui.css" rel="stylesheet" />
    <link href="~/assets/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <script src="~/assets/javascripts/bootstrap.js"></script>
    <script src="~/assets/js/jquery.isotope.js"></script>
   <link href="~/assets/css/style.css" rel="stylesheet" />
    <link href="~/assets/stylesheets/normalize.css" rel="stylesheet" />
    <link href="~/assets/stylesheets/common.css" rel="stylesheet" />
    <link href="~/assets/css/menu.css" rel="stylesheet" />
        <script src="~/assets/javascripts/prefixfree.min.js"></script>
    <link href="~/assets/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
     <link href="~/assets/plugins/font-awesome/css/font-awesome.css" rel="stylesheet" />
        @RenderSection("head", required: false)
     
</head>
<body>
       <div class="content title">
            <div class="row">
                <div class="col-sm-6 ">
                    <img src="~/assets/Images/symphonyLogo.png" width="280" />
                </div>
            </div>
        </div>
        @RenderBody()
    <div class="clearfix"></div>
        @Scripts.Render("~/bundles/jquery")
        @RenderSection("scripts", required: false)
    <script src="~/assets/javascripts/jquery.min.js"></script>
    <script src="~/assets/javascripts/jquery-1.8.2.min.js"></script>
    <script src="~/assets/javascripts/jquery-ui-1.8.24.min.js"></script>
    <script src="~/assets/plugins/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/assets/javascripts/custom.js"></script>
</body>
</html>
