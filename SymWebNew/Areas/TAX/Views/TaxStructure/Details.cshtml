</div>
    <script type="text/javascript">
        $(document).ready(function () {
            SelectAllForDelete();
            $.datepicker.regional[""].dateFormat = 'dd/M/y';
            $.datepicker.setDefaults($.datepicker.regional['']);
            var oTable = $('#myDataTable1').dataTable({
                "bServerSide": true,
                "sAjaxSource": "/TAX/TaxSlab/_index",
                "bProcessing": true,
                "aLengthMenu": tablelength(),
                "bJqueryUI": true,
                "bAutoWidth": false,
                "aoColumns": [
                                {
                                    "sName": "Id",
                                    "bSearchable": false,
                                    "bSortable": false,
                                    "sWidth": "10%",
                                    "sClass": "center",
                                    "fnRender": function (oObj) {
                                        return ' <input onclick="CheckAll(this)" type="checkbox" data-Id="' + oObj.aData[0] + '">&nbsp; <a title="Click on for Edit" style="text-decoration:none;"   href=\"/TAX/TaxSlab/Edit/' + oObj.aData[0] + '\" id="lnkEdit1"  class=" lnkEdit sym-btn-edit-row"></a>' +
                                        '&nbsp; <a title="Click on for Delete" style="text-decoration:none;" data-url="/TAX/TaxSlab/Delete" onclick="deletedData(this, myDataTable,\'' + oObj.aData[0] + '\');"  class="sym-btn-remove-row"></a>';
                                    }
                                }
                                , { "sName": "Step" }
                                , { "sName": "MinRange" }
                                , { "sName": "MaxRange" }
                                , { "sName": "MinTotal" }
                                , { "sName": "MaxTotal" }
                                , { "sName": "Rate" }
                                , { "sName": "CalculateValue" }
                                , { "sName": "Remarks" }
                ]
            });
            oTable.columnFilter({
                sPlaceHolder: "head:after",
                aoColumns: [{ type: "checkbox", "sClass": "chkAll" }
                          , { type: "text" }
                          , { type: "text" }
                          , { type: "text" }
                          , { type: "text" }
                          , { type: "text" }
                          , { type: "text" }
                          , { type: "text" }
                          , { type: "text" }
                          , { type: "text" }
                ]
            });
        });
    </script>
