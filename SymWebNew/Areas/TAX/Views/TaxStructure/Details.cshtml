@{
    ViewBag.Title = "Details";
}
<div class="thinbox">
    <div class="th_thinbox">
        <p>Tax Setup List</p>
    </div>
    <p></p>
    <div class="headrow">
        <button type="button" title="Add New Tax Setup" data-url="/TAX/TaxSetup/Create" id="" class="btnCreate sym-btn-add ">&nbsp;New</button>
        <button type="button" title="Delete Tax Setup" data-url="/TAX/TaxSetup/Delete" onclick="deletedData(this, 'myDataTable');" id="" class="sym-btn-remove">&nbsp;Delete</button>
        <label class="pull-right">Tax Setup</label>
    </div>
    <p></p>
    <div class="row">
        <table id="myDataTable1" class="display">
            <thead>
                <tr>
                    <th class="center">
                        <input type="checkbox" class="chkAll" />Select All</th>
                    <th>Code</th>
                    <th>Name</th>
                    <th>Remarks</th>
                </tr>
                <tr>
                    <th class="center">Actions</th>
                    <th>Code</th>
                    <th>Name</th>
                    <th>Remarks</th>
                </tr>
            </thead>
            <tbody>
            </tbody>
        </table>
    </div>
